.icon {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'XPIcons' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-trocar-baixo:before {
  content: "\e91e";
  color: #fff;
}
.icon-trocar-cima:before {
  content: "\e91f";
  color: #fff;
}
